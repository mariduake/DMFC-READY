FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "120/electrolyte";
    object      V;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0.7;

boundaryField
{
    electrolyteSides
    {
        type            zeroGradient;
    }
    electrolyte_to_fuel
    {
        type            zeroGradient;
    }
    electrolyte_to_air
    {
        type            zeroGradient;
    }
}


